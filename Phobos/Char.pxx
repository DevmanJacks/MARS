// UTF-8 character

export type Char UInt32

export func Char(byte: Byte) -> Char {
    return byte
}

export func Char.IsAlpha() -> Bool {
    if (self >= 65 && self <= 90) || (self >= 97 && self <= 122)
        return true
    
    return false
}

export func Char.IsDigit() -> Bool {
    if self >= 48 && self <= 57
        return true

    return false
}